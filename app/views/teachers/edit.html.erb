<h1>Editing User Information</h1>

<%=form_tag do %>
  <%=label_tag :name %>
  <%=text_field_tag :name, nil, placeholder: @teacher_name%>
  <%=label_tag :email %>
  <%=text_field_tag :email %>
  <%=label_tag :password %>
  <%=password_field_tag :password %>
  <%=submit_tag "Update" %>
<% end %>



<%= link_to 'Back', parents_path %>
